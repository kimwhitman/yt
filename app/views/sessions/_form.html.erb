<div class="returning_login">
  <fieldset>
    <label>
      <%= f.text_field :email  %>
    </label>
    <label>
      <%= f.password_field :password %>
    </label>
    <label class="checkbox">
      <%= f.check_box :remember_me, :checked => true %>
      Remember me. I come here often.
    </label>
  </fieldset>
  <div class="ambassador_unassigned" style="display: none;">
    <p>To credit an Ambassador and begin your 2 week free trial, please enter a valid Ambassador ID:</p>
    <input type="text" class="input"></input>
    <input type="image" class="submit" src="/images/ambassador_button_enter.png"></input>
  </div>
  <div class="ambassador_assigned">
    <h5>Your Ambassador</h5>
    <div class="ambassador_agent_id">
      <span>joinSunny26</span>
    </div>
    <a href="#" class="change_ambassador">Not your Ambassador?</a>
    <p class="checkbox"><input type="checkbox"></input>Notify this Ambassador by e-mail when I subscribe.</p>
  </div>
</div>