    <div id="get_started_ambassador">

      <div class="welcome">
        <h3>Welcome to Yoga Today</h3>
        <p>You've been invited to a free trial membership by Yoga Today Ambassador:</p>

        <% if @ambassador_user %>
          <div class="ambassador_agent_id">
            <span><%= @ambassador_user.ambassador_name %></span>
          </div>
        <% else %>
          <div class="ambassador_assign">
            <input type="text" class="input"></input>
            <input type="image" src="/images/get_started_button_submit.png" class="submit"></input>
          </div>
        <% end %>
      </div>

      <% form_tag new_user_path do %>
        <% if @ambassador_user %>
          <div class="prepaid_selection">
            <h3>Try Yoga Today for 2 weeks</h3>
            <p>Choose one of our prepaid memberships, and your first two weeks are free. Cancel any time.</p>
            <fieldset>
              <label><%= radio_button_tag 'membership', 'Premium Trial', @billing_cycle == 'Premium Trial' %> Yoga Today 365 <span>A full year for just $89.95 - saves you 25%</span></label>
